{
  "name": "@gulali/nodejs-express-sequelize-mysql-api-boilerplate",
  "version": "0.0.1",
  "description": "Nodejs Express Sequelize Mysql API Boilerplate with JWT, Swagger, Winston, PM2",
  "main": "app.js",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=16.16.0"
  },
  "scripts": {
    "lint": "npm run typecheck || true && eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "prepare:dev": "npx sequelize-cli db:create && npx sequelize-cli db:migrate",
    "prepare:prod": "NODE_ENV=production npx sequelize-cli db:create && NODE_ENV=production npx sequelize-cli db:migrate",
    "pm2:dev": "cross-env NODE_ENV=development pm2 start app.js",
    "pm2:test": "cross-env NODE_ENV=test pm2 start app.js",
    "pm2:prod": "cross-env NODE_ENV=production pm2 start app.js",
    "node:dev": "cross-env NODE_ENV=development node app.js",
    "node:test": "cross-env NODE_ENV=test node app.js",
    "node:prod": "cross-env NODE_ENV=production node app.js"
  },
  "bin": {
    "create-nodejs-express-sequelize-mysql-api-boilerplate": "./bin/cli.js"
  },
  "keywords": [
    "api",
    "boilerplate",
    "nodejs",
    "express",
    "sequelize",
    "mysql",
    "jwt",
    "swagger",
    "winston",
    "pm2",
    "api-boilerplate",
    "nodejs-api",
    "nodejs-sequelize-api"
  ],
  "dependencies": {
    "@vscode/sqlite3": "^5.1.10-vscode",
    "axios": "^1.6.0",
    "bcryptjs": "^3.0.2",
    "bignumber.js": "^9.1.2",
    "binance": "^3.1.5",
    "body-parser": "^1.20.1",
    "bull": "^4.16.5",
    "cheerio": "^1.1.2",
    "commander": "^9.4.1",
    "cors": "^2.8.5",
    "dayjs": "^1.11.10",
    "express": "^4.18.2",
    "express-rate-limit": "^8.0.1",
    "helmet": "^6.0.1",
    "http-status": "^1.6.2",
    "https-proxy-agent": "^7.0.6",
    "joi": "^17.9.1",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "morgan": "^1.10.1",
    "mysql2": "^3.14.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "playwright": "^1.57.0",
    "puppeteer": "^24.22.0",
    "request": "^2.88.2",
    "request-ip": "^3.3.0",
    "sequelize": "^6.25.5",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "socks-proxy-agent": "^8.0.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.2",
    "user-agents": "^1.1.617",
    "uuid": "9.0.0",
    "winston": "^3.8.2",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/socket.io-client": "^3.0.0",
    "chai": "^5.2.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "eslint": "^9.39.2",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-plugin-n": "^17.23.1",
    "ignore": "^7.0.5",
    "jest": "^30.0.4",
    "mockjs": "^1.1.0",
    "nodemailer": "^6.9.1",
    "nodemon": "^3.0.1",
    "pm2": "^5.2.2",
    "sequelize-cli": "^6.5.2",
    "typescript": "^5.9.3"
  }
}
